<div class="autosize-input">
  <input
    aria-autocomplete="list"
    autocapitalize="none"
    autocomplete="off"
    autocorrect="off"
    spellcheck="false"
    style={{this.style}}
    tabindex="0"
    type="text"
    value={{this.value}}
    {{on "input" this.onInput}}
    {{ref this "inputRef"}}
  >
</div>
